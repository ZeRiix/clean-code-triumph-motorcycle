{
	"extends": "../tsconfig.json",
	"compilerOptions": {
		"paths": {
			"@routes": [
				"fastify/src/routes/index.ts"
			],
			"@fastify": [
				"fastify/main.ts"
			],
			"@services/*": [
				"fastify/src/services/*"
			],
			"@prisma": [
				"databases/prisma/index.ts"
			],
			"@utils/*": [
				"fastify/src/utils/*"
			],
			"@helpers/*": [
				"fastify/src/helpers/*"
			],
			"@middlewares/*": [
				"fastify/src/middlewares/*"
			],
		}
	},
}