{
	"extends": "../tsconfig.json",
	"compilerOptions": {
		"paths": {
			"@routes": [
				"duplo/src/routes/index.ts"
			],
			"@checkers/*": [
				"duplo/src/checkers/*"
			],
			"@services/*": [
				"duplo/src/services/*"
			],
			"@security/*": [
				"duplo/src/security/*"
			],
			"@utils/*": [
				"duplo/src/utils/*"
			],
			"@schemas/*": [
				"duplo/src/schemas/*"
			],
			"mongoose": [
				"node_modules/mongoose"
			],
			"@prisma": [
				"databases/prisma/index.ts"
			],
			"@mongoose": [
				"databases/mongoose/index.ts"
			],
			"@mongoose/*": [
				"databases/mongoose/schemas/*"
			],
			"@repositories/*": [
				"databases/repositories/*"
			],
			"@envs": [
				"./envs.ts"
			],
		}
	},
}